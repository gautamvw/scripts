    "Version": "2012-10-17",
    "Statement": {
        "Effect": "Allow",
        "Resource": "arn:aws:iam::123456789012:role/somerole",
        "Action": "sts:AssumeRole"
    }
}

#Command to assume role - Used in Jenkins STS Job
#aws sts assume-role --role-arn arn:aws:iam::123456789012:role/somerole --role-session-name stsexample
